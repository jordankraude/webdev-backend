{
  "swagger": "2.0",
  "info": {
    "title": "WebDev Forms API",
    "description": "API to handle form submits from web developer page",
    "version": "1.0.0"
  },
  "host": "webdev-backend-n0v3.onrender.com",
  "basePath": "/",
  "schemes": [
    "https"
  ],
  "paths": {
    "/api-docs": {
      "get": {
        "description": "",
        "parameters": [],
        "responses": {}
      }
    },
    "/quote-request/": {
      "post": {
        "description": "Submit a quote request with optional file upload",
        "parameters": [
          {
            "name": "firstName",
            "in": "formData",
            "type": "string",
            "example": "any",
            "required": true
          },
          {
            "name": "lastName",
            "in": "formData",
            "type": "string",
            "example": "any",
            "required": true
          },
          {
            "name": "email",
            "in": "formData",
            "type": "string",
            "example": "any",
            "required": true
          },
          {
            "name": "phoneNumber",
            "in": "formData",
            "type": "string",
            "example": "any",
            "required": true
          },
          {
            "name": "budget",
            "in": "formData",
            "type": "string",
            "example": "< $1,000",
            "required": true,
            "enum": ["< $1,000", "$1,000 - $5,000", "$5,000 - $10,000", "$10,000 - $15,000", "> $15,000"]
          },
          {
            "name": "otherInfo",
            "in": "formData",
            "type": "string",
            "example": "any"
          },
          {
            "name": "file",
            "in": "formData",
            "type": "file"
          }
        ],
        "consumes": ["multipart/form-data"],
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    },
    "/feedback/": {
      "post": {
        "description": "",
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "schema": {
              "type": "object",
              "properties": {
                "firstName": {
                  "example": "any",
                  "required": true
                },
                "lastName": {
                  "example": "any",
                  "required": true
                },
                "email": {
                  "example": "any",
                  "required": true
                },
                "feedback": {
                  "example": "any",
                  "required": true
                },
                "likeResponse": {
                  "example": "any",
                  "required": true
                }
              }
            }
          }
        ],
        "consumes": ["multipart/form-data"],
        "responses": {
          "201": {
            "description": "Created"
          },
          "500": {
            "description": "Internal Server Error"
          }
        }
      }
    }
  }
}